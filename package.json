    "cookie-parser": "^1.4.6",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongoose": "^8.2.1",
    "morgan-body": "^2.6.9",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0"
  }
}
